<!--pages/onlineSearchFour/onlineSearchFour.wxml-->
<view class="page">
  <view class="weui-msg">
    <view class="weui-msg__icon-area">
      <icon type="success" size="93"></icon>
    </view>
    <view class="weui-msg__text-area">
      <view class="weui-msg__title">登记成功</view>
      <view class="weui-msg__desc"></view>
    </view>
    <view class="weui-msg__opr-area">
      <view class="weui-btn-area">
        <button class="weui-btn" type="primary" bindtap="submit">确认查档</button>
        <button class="weui-btn" type="default" bindtap="backHome">放弃查档</button>
      </view>
    </view>
  </view>
</view>